function [F_matrix] = estimate_fund_matrix(matches)

% create matrix A

A=ones(size(matches,1),9);
A(:,1)=matches(:,1).*matches(:,3);
A(:,2)=matches(:,1).*matches(:,2);
A(:,3)=matches(:,1);
A(:,4)=matches(:,3).*matches(:,2);
A(:,5)=matches(:,2).*matches(:,4);
A(:,6)=matches(:,2);
A(:,7)=matches(:,3);
A(:,8)=matches(:,4);
[U,S,V]=svd(A);
[M,I]=min(diag(S));
f=V(I,:);

F_matrix1=reshape(f,3,3);

[U1,S1,V1]=svd()
disp('f');

end

